<mod:findName type="event" name="STORAGE_CHECK_BUTTON">
	<mod:findWithChildLike type="choice" child-type="text" panic="true">
        <mod:selector>Modify a modular weapon.</mod:selector>
        <mod:removeTag />
    </mod:findWithChildLike>
	<mod-append:choice req="HEKTAR_MODULAR_WEAPONS_STORAGE" hidden="true" blue="false">
        <text>Modify a modular weapon.</text>
        <event load="STORAGE_CHECK_MODULAR"/>
    </mod-append:choice>

</mod:findName>

<mod:findName type="event" name="STORAGE_CHECK_JOB_FLEET">

	<mod-append:choice req="HEKTAR_MODULAR_MINELAUNCHER_DELAY" hidden="true" blue="false">
        <text>Use your Modular Mine Launcher. [-13}, gives 2 delay]</text>
        <event>
			<text>You rig the ammunition and set a trap for the Rebels. It's definitely illegal to do, but you'll be gone before it matters.</text>
		   <modifyPursuit amount="-2"/>
		   <item_modify>
			   <item type="missiles" min="-13" max="-13"/>
		   </item_modify>
		   <choice hidden="true" req="loc_mine_counter" max_group="0" lvl="0" blue="false">
			   <text>Continue...</text>
			   <event load="TRAP_WARNING_1"/>
		   </choice>
		   <choice hidden="true" req="loc_mine_counter" max_group="0" lvl="1" blue="false">
			   <text>Continue...</text>
			   <event load="TRAP_WARNING_2"/>
		   </choice>
		   <choice hidden="true" req="loc_mine_counter" max_group="0" lvl="2" blue="false">
			   <text>Continue...</text>
			   <event load="TRAP_WARNING_3"/>
		   </choice>
		   <choice hidden="true" req="loc_mine_counter" max_group="0" lvl="3" blue="false">
			   <text>Continue...</text>
			   <event load="MORALITY_UPDATE_GENERAL"/>
		   </choice>
	   </event>
    </mod-append:choice>
	<mod-append:choice req="HEKTAR_MODULAR_MINELAUNCHER_DELAY_POWER" hidden="true" blue="false">
        <text>Use your Modular Mine Launcher (Power Module). [-10}, gives 2 delay]</text>
        <event>
			<text>You rig the ammunition and set a trap for the Rebels. It's definitely illegal to do, but you'll be gone before it matters.</text>
		   <modifyPursuit amount="-2"/>
		   <item_modify>
			   <item type="missiles" min="-10" max="-10"/>
		   </item_modify>
		   <choice hidden="true" req="loc_mine_counter" max_group="0" lvl="0" blue="false">
			   <text>Continue...</text>
			   <event load="TRAP_WARNING_1"/>
		   </choice>
		   <choice hidden="true" req="loc_mine_counter" max_group="0" lvl="1" blue="false">
			   <text>Continue...</text>
			   <event load="TRAP_WARNING_2"/>
		   </choice>
		   <choice hidden="true" req="loc_mine_counter" max_group="0" lvl="2" blue="false">
			   <text>Continue...</text>
			   <event load="TRAP_WARNING_3"/>
		   </choice>
		   <choice hidden="true" req="loc_mine_counter" max_group="0" lvl="3" blue="false">
			   <text>Continue...</text>
			   <event load="MORALITY_UPDATE_GENERAL"/>
		   </choice>
	   </event>
    </mod-append:choice>

</mod:findName>

